<property xmlns="http://faculty.washington.edu/tgis/xml/xsd/modules/xsd4md/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xsi:schemaLocation="http://faculty.washington.edu/tgis/xml/xsd/modules/xsd4md/ http://faculty.washington.edu/tgis/xml/xsd/modules/xsd4md/schema-4-properties-0.1.xsd">
    <labels>
        <platformIndependent>studioLocation</platformIndependent>
        <dc>none</dc>
    </labels>
    <cdm>
        <label>Studio Location</label>
        <cdmDatatype>text</cdmDatatype>
        <cdmLarge>no</cdmLarge>
        <searchable>no</searchable>
        <hidden>yes</hidden>
        <required>no</required>
        <cdmControlledVocab>no</cdmControlledVocab>
    </cdm>
    <dspace>
        <dspaceProperty/>
    </dspace>
    <descriptions>
        <definition co="no">
            <para>If the city of publication in which the photographer/publisher worked at the time the image was printed is noted on the front of the photograph, or is known to the cataloger, it is provided in Studio Location field.</para>
        </definition>
        <instructions co="no">
            <para>Input in standard hierarchical form separated by a dash (equivalent to two hyphens).</para>
            <para>If there is more than one studio location noted, separate them by semi-colons. </para>
        </instructions>
        
    </descriptions>
    <examples>
        <example co="no">
            <para>United States—Washington (State)—Seattle</para>
            <para>Canada—Yukon Territory—Dawson</para>
        </example>
        
        <!-- customization here is flawed.
             I think we need
             <customizations>
                 <description | example>
                     <para @co @dd @paraDec @paraEx> REPEATABLE
                         <para>
        -->
        
    </examples>
    <additionalInfo>
        <admin>
            <para/>
        </admin>
        <generalNotes>
            <para/>
        </generalNotes>
    </additionalInfo>
    <marc>
        <dcgMarcMapping/>
        <marcNotes>
            <para/>
        </marcNotes>
    </marc>
    <xsd>
        <typeAttribute>xs:string</typeAttribute>
        <complexType/>
        
    </xsd>
    <rdf>
        <sameAs/>
    </rdf>
    <history>
        <dateCreated>
            
            <range>
                <start>2000-01-01</start>
                <end>2017-01-12</end>
            </range>
        </dateCreated>
        <created4Coll>Special Collections Photograph Collections</created4Coll>
        <justification>Special Collections believed representing studio location was useful information, so we created this property.</justification>
        <changeLog>
            <change />
        </changeLog>
    </history>
</property>