<property xmlns="http://faculty.washington.edu/tgis/schemasProject/xsd4md"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xsi:schemaLocation="http://faculty.washington.edu/tgis/schemasProject/xsd4md http://faculty.washington.edu/tgis/schemasProject/xsd4md/schema-4-properties-0.1.xsd">
    <labels>
        <platformIndependent>contributor</platformIndependent>
        <dc>contributor</dc>
    </labels>
    <cdm>
        <label>Contributor</label>
        <cdmDatatype>text</cdmDatatype>
        <cdmLarge>no</cdmLarge>
        <searchable>no</searchable>
        <hidden>yes</hidden>
        <required>no</required>
        <cdmControlledVocab>no</cdmControlledVocab>
    </cdm>
    <dspace>
        <dspaceProperty/>
    </dspace>
    <descriptions>
        <definition>
            <para>A person or organization not specified in the Creator field or Photographer field who has made a significant contribution to the resource, but whose contribution is secondary to any person or organization specified in the Creator field.</para>
        </definition>
        <instructions co="no">
            <para>Input name of the contributor.</para>
            <para>Use LC Authority File for form of name, if available.</para>
            <para>Input Lastname, Firstname for all personal names.</para>
        </instructions>
        <instructions co="object">
            <para>Input name of the contributor.</para>
            <para>Use LC Authority File for form of name, if available.</para>
            <para>Input Lastname, Firstname for all personal names.</para>
        </instructions>
    </descriptions>
    <examples>
        <example co="no">
            <para>University of Washington Libraries, Special Collections Division</para>
        </example>
        <example co="object">
            <para>University of Washington Libraries, Special Collections Division</para>
        </example>
    </examples>
    <additionalInfo/>
    <marc>
        <dcgMarcMapping/>
        <marcNotes>
            <para/>
        </marcNotes>
    </marc>
    <xsd>
        <typeAttribute>xs:string</typeAttribute>
        <complexType/>
    </xsd>
    <rdf>
        <sameAs/>
    </rdf>
    <history>
        <dateCreated>
            <range>
                <start>2000-01-01</start>
                <end>2017-01-12</end>
            </range>
        </dateCreated>
        <created4Coll>Special Collections Photograph Collections</created4Coll>
        <justification>Special Collections wanted to use this property for describing the contribution of the University of Washington Libraries.</justification>
        <changeLog>
            <change date="2017-07-03">Changed in accordance with changes to schema.</change>
        </changeLog>
    </history>
</property>