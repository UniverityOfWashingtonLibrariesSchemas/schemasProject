        <hidden>no</hidden>
        <required>no</required>
        <cdmControlledVocab>yes</cdmControlledVocab>
    </cdm>
    <dspace>
        <dspaceProperty/>
    </dspace>
    <descriptions>
        <definition>
            <para>The topic of the resource.</para>
        </definition>
        <instructions co="no">
            <para>Terms in this field are taken from the LC Thesaurus for Graphic Materials.</para>
            <para>Separate multiple headings with a semi-colon.</para>
        </instructions>
        <instructions co="object">
            <para>Terms in this field are taken from the LC Thesaurus for Graphic Materials.</para>
            <para>Separate multiple headings with a semi-colon.</para>
        </instructions>
    </descriptions>
    <examples>
        <example co="no">
            <para>Cities &amp; towns--Alaska</para>
        </example>
        <example co="object">
            <para>Cities &amp; towns--Alaska</para>
        </example>
    </examples>
    <additionalInfo>
    </additionalInfo>
    <marc>
        <dcgMarcMapping/>
        <marcNotes>
            <para/>
        </marcNotes>
    </marc>
    <xsd>
        <typeAttribute>xs:string</typeAttribute>
        <complexType/>
        
    </xsd>
    <rdf>
        <sameAs/>
    </rdf>
    <history>
        <dateCreated>
            
            <range>
                <start>2000-01-01</start>
                <end>2017-01-06</end>
            </range>
        </dateCreated>
        <created4Coll>Special Collections Photograph Collections</created4Coll>
