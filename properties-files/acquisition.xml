<property xmlns="http://faculty.washington.edu/tgis/schemasProject/xsd4md"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xsi:schemaLocation="http://faculty.washington.edu/tgis/schemasProject/xsd4md http://faculty.washington.edu/tgis/schemasProject/xsd4md/schema-4-properties-0.1.xsd">
    <labels>
        <platformIndependent>acquisition</platformIndependent>
        <dc>none</dc>
    </labels>
    <cdm>
        <label>Acquisition</label>
        <cdmDatatype>text</cdmDatatype>
        <cdmLarge>no</cdmLarge>
        <searchable>no</searchable>
        <hidden>yes</hidden>
        <required>no</required>
        <cdmControlledVocab>no</cdmControlledVocab>
    </cdm>
    <dspace>
        <dspaceProperty/>
    </dspace>
    <descriptions>
        <definition>
            <para>Source and date of acquisition of the resource.</para>
        </definition>
        <instructions co="no">
            <para>Input text describing the source and date of acquisition of the resource.</para>
        </instructions>
        <instructions co="object">
            <para>Input text describing the source and date of acquisition of the resource.</para>
        </instructions>
    </descriptions>
    <examples>
        <example co="no">
            <para>Purchased from Fairlook Antiques, Seattle, 1989</para>
            <para>Gift of Aubrey Watzek, 1953; acc. no. 53-1634</para>
        </example>
        <example co="object">
            <para>Purchased from Fairlook Antiques, Seattle, 1989</para>
            <para>Gift of Aubrey Watzek, 1953; acc. no. 53-1634</para>
        </example>
        
        <!-- customization here is flawed.
             I think we need
             <customizations>
                 <description | example>
                     <para @co @dd @paraDec @paraEx> REPEATABLE
                         <para>
        -->
        
    </examples>
    <additionalInfo>
        <admin co="all">
            <para>Some collections have mysterious values for this property; for example, the only value for this property in ayp is "KK."</para>
        </admin>
    </additionalInfo>
    <marc>
        <dcgMarcMapping/>
        <marcNotes>
            <para/>
        </marcNotes>
    </marc>
    <xsd>
        <typeAttribute>xs:string</typeAttribute>
        <complexType/>
        
    </xsd>
    <rdf>
        <sameAs/>
    </rdf>
    <history>
        <dateCreated>
       
            <range>
                <start>2000-01-01</start>
                <end>2017-02-21</end>
            </range>
        </dateCreated>
        <created4Coll>Special Collections Photograph Collections</created4Coll>
        <justification>Uncertain; Special Collections wanted to enter information on how selected images were acquired (but it's not clear why they would want to input that information in CONTENTdm).</justification>
        <changeLog>
            <change date="2017-07-03">Changed date in accordance with schema changes.</change>
        </changeLog>
    </history>
</property>