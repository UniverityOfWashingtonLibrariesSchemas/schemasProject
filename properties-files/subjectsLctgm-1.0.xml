<property xmlns="http://faculty.washington.edu/tgis/xml/xsd/modules/xsd4md/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xsi:schemaLocation="http://faculty.washington.edu/tgis/xml/xsd/modules/xsd4md/ http://faculty.washington.edu/tgis/xml/xsd/modules/xsd4md/schema-4-properties-0.1.xsd">
    <labels>
        <platformIndependent>subjectsLctgm</platformIndependent>
        <dc>subject</dc>
    </labels>
    <cdm>
        <label>Subjects (LCTGM)</label>
        <cdmDatatype>text</cdmDatatype>
        <cdmLarge>no</cdmLarge>
        <searchable>yes</searchable>
        <hidden>no</hidden>
        <required>no</required>
        <cdmControlledVocab>yes</cdmControlledVocab>
    </cdm>
    <dspace>
        <dspaceProperty/>
    </dspace>
    <descriptions>
        <definition co="no">
            <para>Terms in this field are taken from the LC Thesaurus for Graphic Materials.</para>
        </definition>
        <instructions co="no">
            <para>Enter subject terms and phrases as a list, with no blank lines between terms.</para>
        </instructions>
        
    </descriptions>
    <examples>
        <example co="no">
            <para>Cities &amp; towns--Alaska</para>
        </example>
        
        <!-- customization here is flawed.
             I think we need
             <customizations>
                 <description | example>
                     <para @co @dd @paraDec @paraEx> REPEATABLE
                         <para>
        -->
        
    </examples>
    <additionalInfo>
        <admin>
            <para/>
        </admin>
        <generalNotes>
            <para/>
        </generalNotes>
    </additionalInfo>
    <marc>
        <dcgMarcMapping/>
        <marcNotes>
            <para/>
        </marcNotes>
    </marc>
    <xsd>
        <typeAttribute>xs:string</typeAttribute>
        <complexType/>
        
    </xsd>
    <rdf>
        <sameAs/>
    </rdf>
    <history>
        <dateCreated>
            
            <range>
                <start>2000-01-01</start>
                <end>2017-01-06</end>
            </range>
        </dateCreated>
        <created4Coll>Special Collections Photograph Collections</created4Coll>
        <justification>We wanted a subject field that distinguishes TGM headings from other subject headings, to get the full benefits of using a controlled vocabulary.</justification>
        <changeLog>
            <change/>
        </changeLog>
    </history>
</property>