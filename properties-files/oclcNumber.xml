<property xmlns="http://faculty.washington.edu/tgis/xml/xsd/modules/xsd4md/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xsi:schemaLocation="http://faculty.washington.edu/tgis/xml/xsd/modules/xsd4md/ http://faculty.washington.edu/tgis/xml/xsd/modules/xsd4md/schema-4-properties-0.1.xsd">
    <labels>
        <platformIndependent>oclcNumber</platformIndependent>
        <dc>none</dc>
    </labels>
    <cdm>
        <label>oclc Number</label>
        <cdmDatatype>text</cdmDatatype>
        <cdmLarge>no</cdmLarge>
        <searchable>no</searchable>
        <hidden>yes</hidden>
        <required>no</required>
        <cdmControlledVocab>no</cdmControlledVocab>
    </cdm>
    <dspace>
        <dspaceProperty/>
    </dspace>
    <descriptions>
        <definition co="all">
            <para>Unique identifier for the item in OCLC WorldCat. Assigned by OCLC.</para>
        </definition>
        <instructions co="no">
            <para>Do not input. Automatically created by CONTENTdm.</para>
        </instructions>
        
    </descriptions>
    <examples>
        <example co="no">
            <para>18994654</para>
        </example>
        
        <!-- customization here is flawed.
             I think we need
             <customizations>
                 <description | example>
                     <para @co @dd @paraDec @paraEx> REPEATABLE
                         <para>
        -->
        
    </examples>
    <additionalInfo>
        <admin>
            <para>CONTENTdm administrative field.</para>
            <para>Does NOT output in OAI-PMH.</para>
        </admin>
        <generalNotes>
            <para/>
        </generalNotes>
    </additionalInfo>
    <marc>
        <dcgMarcMapping/>
        <marcNotes>
            <para/>
        </marcNotes>
    </marc>
    <xsd>
        <simpleType>
            <xs:union>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:length value="0"/>
                    </xs:restriction>
                </xs:simpleType>
                <xs:simpleType>
                    <xs:restriction base="xs:integer"/>
                </xs:simpleType>
            </xs:union>
        </simpleType>
    </xsd>
    <rdf>
        <sameAs/>
    </rdf>
    <history>
        <dateCreated>
            <single>2006-01-01</single>
            
        </dateCreated>
        <created4Coll>CONTENTdm</created4Coll>
        <justification>Field to place the OCLC Number when record is added to WorldCat via the OCLC Digital Collection Gateway.</justification>
        <changeLog>
            <change/>
        </changeLog>
    </history>
</property>